{
    "name": "serroba/brighte",
    "description": "A Slim Framework skeleton application for rapid development",
    "homepage": "http://github.com/serroba/brighte",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Machuca",
            "email": "serroba@gmail.com",
            "homepage": "http://twitter.com/serroba"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "eloquent/enumeration": "^6.0",
        "league/fractal": "^0.18.0",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^0.5",
        "slim/slim": "^4.1",
        "symfony/event-dispatcher": "^4.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.5",
        "bigcommerce/mock-injector": "^3.0"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
